include		.makedep

dependExtra:
	mkdir -p $(BLD_LIB_DIR)

cleanExtra:
	rm -f *.o *.so *.dylib *.a *.dll *.lib *.exe *.manifest
	rm -f ejswebserver.conf mime.types
	rm -f $(BLD_LIB_DIR)/*.o $(BLD_LIB_DIR)/*.so $(BLD_LIB_DIR)/*.dylib $(BLD_LIB_DIR)/*.a 
	rm -f $(BLD_LIB_DIR)/*.dll $(BLD_LIB_DIR)/*.lib
	rm -f $(BLD_LIB_DIR)/*.conf $(BLD_LIB_DIR)/*.types
	rm -rf default-web $(BLD_LIB_DIR)/default-web

clobberExtra: cleanExtra
	[ "$(BUILD_CROSS)" = 1 ] && rm -fr "./$(BLD_HOST_SYSTEM)" ; true

#
#   Local variables:
#   tab-width: 4
#   c-basic-offset: 4
#   End:
#   vim: sw=4 ts=4 noexpandtab
#
