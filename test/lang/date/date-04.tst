var i = 0
for (y = 2500; y > 1000; y -= 10) {
    d = new Date(y, 0, 1)
    offset = d - 0
    line = offset + " " + y + " " + d
    // print(line)
    assert(line.replace(/:00 .*/, ":00") == results[i++])
}

d = new Date

if (d.format) {
    if (d.format("%Z") == "PDT") {
        assert(d.getTimezoneOffset() == 420)
    } else if (d.format("%Z") == "PST") {
        assert(d.getTimezoneOffset() == 480)
    }
}

